select TIMESTAMP , VALUE from ( select TIMESTAMP , VALUE , DRIVE_NAME from (select char(varchar_format(START_TIME,'YYYY-MM-DD HH24:MI:SS'),19) TIMESTAMP, 1 VALUE , DRIVE_NAME from summary where activity='TAPE MOUNT') UNION (select char(varchar_format(END_TIME,'YYYY-MM-DD HH24:MI:SS'),19) TIMESTAMP, -1 as VALUE , DRIVE_NAME from summary where activity='TAPE MOUNT')) order by 1
