select char(varchar_format(START_TIME,'YYYY-MM-DD HH24:MI:SS'),19) TIMESTAMP, cast(((BYTES)/timestampdiff(2,char(END_TIME-START_TIME+1)))/1024.0/1024.0 as decimal(9,2)) as MBS ,  char(ACTIVITY,40)   from summary where BYTES<> 0 and ACTIVITY  not in ('ARCHIVE','BACKUP','DELETE FILESPACE','EXPIRATION','EXPORT','PROCESS_END','PROCESS_START','RESTORE','RETRIEVE','SERVER_INIT','SUR_ARCH_OCCUPANCY','SUR_OCCUPANCY','SUR_RET_OCCUPANCY','TAPE MOUNT','FULL_DBBACKUP','MOVE DATA') UNION ALL select char(varchar_format(END_TIME,'YYYY-MM-DD HH24:MI:SS'),19) TIMESTAMP, cast(((-BYTES)/timestampdiff(2,char(END_TIME-START_TIME+1)))/1024.0/1024.0 as decimal(9,2)) as MBS,  char(ACTIVITY, 40 ) as TASK from summary where BYTES<> 0 and ACTIVITY  not in ('ARCHIVE','BACKUP','DELETE FILESPACE','EXPIRATION','EXPORT','PROCESS_END','PROCESS_START','RESTORE','RETRIEVE','SERVER_INIT','SUR_ARCH_OCCUPANCY','SUR_OCCUPANCY','SUR_RET_OCCUPANCY','TAPE MOUNT','FULL_DBBACKUP','MOVE DATA') order by 1

